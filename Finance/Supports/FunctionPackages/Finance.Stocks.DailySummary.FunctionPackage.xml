<?xml version="1.0"?>
<FunctionPackage xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Finance.Stocks.DailySummary</Name>
  <Domain>Finance.Stocks.DailySummary</Domain>
  <Description>DailySummary功能包</Description>
  <BasePackageNames>
    <Name>Finance.Common</Name>
  </BasePackageNames>
  <Items>
    <FunctionPoint>
      <Name>Finance.Stocks.DailySummary缓存服务。</Name>
      <Description></Description>
      <AssemblyQualifiedName>Finance.Stocks.DailySummary.Services.DailySummaryCacheService,Finance.Stocks.DailySummary</AssemblyQualifiedName>
    </FunctionPoint>
    <FunctionPoint>
      <Name>DailySummaryData备份服务。</Name>
      <Description></Description>
      <AssemblyQualifiedName>Finance.Stocks.DailySummary.Services.DailySummaryBackupService,Finance.Stocks.DailySummary</AssemblyQualifiedName>
    </FunctionPoint>
    <FunctionPoint>
      <Name>StockInfo备份完成后记录日志。</Name>
      <Description></Description>
      <AssemblyQualifiedName>Finance.Stocks.StockInfo.Interceptors.OnAfterBackupWriteLog,Finance.Stocks.StockInfo</AssemblyQualifiedName>
    </FunctionPoint>
